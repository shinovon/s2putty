TARGET		puttyengine.dll
TARGETTYPE	dll
SOURCEPATH	..\..\engine
#ifdef EKA2
CAPABILITY	NetworkServices
UID		0x1000008d 0xf01f9073
#else
UID		0x1000008d 0x101f9073
#endif

USERINCLUDE	..\..\include ..\..\engine ..\..\engine\puttysymbian 
USERINCLUDE	..\..\engine\putty ..\..\engine\putty\charset

